{
  "name": "2020-live-illustration",
  "version": "0.0.1",
  "description": "Interactive illustration.",
  "scripts": {
    "lint": "tsc --noEmit && eslint --quiet '**/*.{js,ts}'",
    "lint:fix": "eslint --fix '**/*.{js,ts}'",
    "lintCss": "prettier --check 'src/**/*.css'",
    "lintCss:fix": "prettier --write 'src/**/*.css'",
    "test": "vitest",
    "start": "vite",
    "preview": "vite preview",
    "build": "vite build"
  },
  "author": "Oskar Wrobel <kontakt@oskarwrobel.pl> (https://www.oskarwrobel.pl/)",
  "dependencies": {
    "gsap": "^3.12.2",
    "lodash-es": "^4.17.21",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.11",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.1.0",
    "postcss-nesting": "^12.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-svgo": "^1.3.0",
    "vitest": "^0.34.6"
  },
  "lint-staged": {
    "**/*.(ts|js)": [
      "eslint --quiet"
    ]
